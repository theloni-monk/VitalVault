{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Vt2devbuild",
        "width": 800
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "eas build --platform all",
    "beforeDevCommand": "npx expo start -c",
    "devUrl": "http://localhost:8081",
    "frontendDist": "../build"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "com.tauri.dev",
  "plugins": {
    "typegen": {
      "excludePatterns": null,
      "includePatterns": null,
      "includePrivate": false,
      "outputPath": "./src/generated",
      "projectPath": ".",
      "typeMappings": null,
      "validationLibrary": "none",
      "verbose": false,
      "visualizeDeps": false
    }
  },
  "productName": "vitalvault",
  "version": "0.1.0"
}